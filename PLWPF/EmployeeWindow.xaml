<Window x:Class="PLWPF.AddEmployee"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PLWPF"
        mc:Ignorable="d"
        Title="Employee" Height="300" Width="300">
    <Grid>
        
        <Button x:Name="Add_Employee_Botton" Content="Add Employee" HorizontalAlignment="Left" Height="20" Margin="168,40,0,0" VerticalAlignment="Top" Width="100"/>
        <Button x:Name="Update_Employee_Botton" Content="Update Employee" HorizontalAlignment="Left" Height="21" Margin="168,128,0,0" VerticalAlignment="Top" Width="100"/>
        <Button x:Name="Delete_Employee_Botton" Content="Delete Employee" HorizontalAlignment="Left" Height="21" Margin="168,85,0,0" VerticalAlignment="Top" Width="100"/>
       
    </Grid>
</Window>
